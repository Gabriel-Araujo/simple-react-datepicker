{
  "compilerOptions": {
    "outDir": "dist",
    "module": "ESNext",
    "target": "ES2020",
    "lib": ["ES2020", "ES2024", "ES5", "DOM"],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "declaration": true,
    "moduleResolution": "bundler",
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src", "vite.config.ts"],
  "exclude": ["node_modules", "dist", "example"],
  "references": [  ],
}
